{
  "flex-layout.row#department__results": {
    "title": "Department Results container",
    "children": [
      /* "filter-navigator.v3", */
      "gallery-layout-switcher",
      "search-content#search__result--container"
    ],
    "props": {
      "colSizing": "auto",
      "blockClass": "department__results--container"
    }
  },
  "search-content#search__result--container": {
    "title": "Container search result",
    "blocks": [
      "gallery#search__result--gallery",
      "not-found"
    ],
    "props": {
      "blockClass": "search__result--content"
    }
  },
  "gallery#search__result--gallery": {
    "title": "Search result gallery",
    "props": {
      "blockClass": "search__result--gallery",
      "layouts": [
        {
          "name": "main-grid",
          "component": "MainGrid",
          "itemsPerRow": {
            "desktop": 3,
            "tablet": 2,
            "phone": 1
          }
        },
        {
          "name": "main-list",
          "component": "MainList",
          "itemsPerRow": 1
        }
      ],
      "MainGrid": "product-summary.shelf#plp__grid--products",
      "MainList": "product-summary.shelf#home__list--product"
    }
  },
  "product-summary.shelf#plp__grid--products": {
    "title": "Plp product grid",
    "children": [
      "slider-layout#plp__slider--image",
      "stack-layout#global__plus--available",
      "stack-layout#global__section--add_to_cart",
      "product-summary-name#plp__product--name",
      "product-summary-brand#plp__product--brand",
      "product-summary-price#plp__product--price",
      "flex-layout.row#plp__produc--sku",
      /*"product-summary-space",*/
      /* "flex-layout.row#plp__product--dots", */
      "add-to-cart-button#plp__product--button"
    ],
    "props": {
      "blockClass": "plp__grid--products"
    }
  },
  "slider-layout#plp__slider--image": {
    "props": {
      "itemsPerPage": {
        "desktop": 1,
        "tablet": 1,
        "phone": 1
      },
      "infinite": true,
      "showNavigationArrows": "desktopOnly",
      "blockClass": "plp__slider--image"
    },
    "children": [
      "product-summary-image#plp__product--image",
      "product-summary-image#plp__product--image",
      "product-summary-image#plp__product--image"
    ]
  },
  "product-summary-image#plp__product--image": {
    "title": "Product list page image",
    "props": {
      "blockClass": "plp__product--image",
      "aspectRatio": "1:1"
    }
  },
  "stack-layout#global__plus--available": {
    "title": "Global plus size available stack",
    "children": [
      "flex-layout.row#global__plus--available"
    ],
    "props": {
      "blockClass": "global__plus--available"
    }
  },
  "flex-layout.row#global__plus--available": {
    "title": "Global plus size available container",
    "children": [
      "rich-text#global__plus--available"
    ],
    "props": {
      "blockClass": "global__plus--available"
    }
  },
  "rich-text#global__plus--available": {
    "title": "Global plus size available text",
    "props": {
      "text": "Disponible plus",
      "blockClass": "global__plus--available"
    }
  },
  "stack-layout#global__section--add_to_cart": {
