{
  "sticky-layout#desktop": {
    "title": "Sticky header",
    "props": {
      "blockClass": "sticky-layout#desktop__header"
    },
    "children": [
      "flex-layout.row#desktop__header--free_shipping",
      "flex-layout.row#desktop__header--menu"
    ]
  },

  "flex-layout.row#desktop__header--menu": {
    "title": "Menu header section on desktop",
    "props": {
      "horizontalAlign": "rigth",
      "verticalAlign": "center",
      "preventHorizontalStretch": true,
      "preventVerticalStretch": true,
      "fullWidth": true,
      "blockClass": "flex-layout__desktop"
    },
    "children": [
      "vtex.menu@2.x:menu#desktop__menu--category",
      "flex-layout.col#desktop__menu--spacer",
      "flex-layout.col#logo-desktop",
      "flex-layout.col#desktop__menu--spacer",
      "flex-layout.row#desktop__header--icons"
      /* "flex-layout.row#desktop__searchbar--one",
      "flex-layout.col#desktop__menu--login",
      "flex-layout.col#desktop__menu--favorites",
      "flex-layout.col#desktop__menu--minicart"*/
    ]
  },

  //Menu section
  "vtex.menu@2.x:menu#desktop__menu--category": {
    "title": "Main section menu desktop",
    "props": {
      "experimentalOptimizeRendering": true,
      "blockClass": "desktop__menu--category"
    },
    "children": [
      "menu-item#desktop__menu--women",
      "menu-item#desktop__menu--men",
      "menu-item#desktop__menu--teens",
      "menu-item#desktop__menu--kids"
    ]
  },

  //Women section
  "menu-item#desktop__menu--women": {
    "title": "Women section",
    "props": {
      "id": "menu-item-women",
      "type": "custom",
      "iconId": null,
      "highlight": false,
      "blockClass": "header__menu--department_name",
      "itemProps": {
        "type": "internal",
        "href": "#",
        "noFollow": true,
        "tagTitle": "women",
        "text": "Mujer"
      }
    },
    "blocks": [
      "vtex.menu@2.x:submenu#desktop__submenu--women"
    ]
  },

  //Men section
