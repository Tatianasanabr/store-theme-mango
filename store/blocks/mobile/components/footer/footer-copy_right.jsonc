    "flex-layout.col#mobile__footer--policy": {
        "title": "mobile footer policy container",
        "children": [
            "rich-text#mobile__policy--text"
        ],
        "props": {
            "blockClass": "mobile__footer--policy"
        }
    },
    "rich-text#mobile__policy--text": {
        "title": "mobile copyright text",
        "props": {
            "text": "Política de Privacidad y Cookies | [Condiciones de Venta](https://shop.mango.com/co/helppopup/6570.html)",
