        "title": "mobile footer copyright container",
        "children": [
            "flex-layout.col#mobile__footer--rights",
            "flex-layout.col#mobile__footer--policy"
        ],
        "props": {
            "blockClass": "mobile__footer--copyright"
        }
    },
    "flex-layout.col#mobile__footer--rights": {
        "title": "mobile footer rights container",
        "children": [
            "rich-text#mobile__copyright--text"
        ],
        "props": {
            "blockClass": "mobile__footer--rights"
        }
    },
    "rich-text#mobile__copyright--text": {
        "title": "mobile copyright text",
